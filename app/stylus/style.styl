body
    height 1500px
.disable_select
    -webkit-touch-callout none
    -webkit-user-select none
    -khtml-user-select none
    -moz-user-select none
    -ms-user-select none
    user-select none
.bg1
    position fixed
    top -10%
    left -10%
    min-height 75%
    height 75%
    z-index -100
.dark
    background-color rgba(0, 0, 0, 0.5)
    position absolute
    top 0
    left 0
    width 100%
    height 100%
    z-index -50
.page-one-windows-apps__img
    position absolute
    top 20%
    left 50%
    margin-left -400px
    display block
    box-shadow 2px 5px 25px #000
.page-one-title__h1
    font-family "Roboto"
    font-weight 700
    font-size 5em
    color white
    margin 0
    text-shadow 2px 2px 8px #000
    z-index 1000
.page-one__section
    display table
    position absolute
    top 27%
    vertical-align middle
    text-align center
    width 100%
    z-index 1000
.page-one-text__h3
    font-family "Roboto Condensed"
    font-weight 300
    font-style italic
    font-size 1.5em
    text-align center
    color white
    margin 0
    text-shadow 2px 2px 1px #000
    z-index 1000
    padding-bottom 2em
.page-one-office-logo__img
    width 200px
    float right
    margin-right 120px
    margin-top 100px
    display block
.page-one-svfu-logo__img
    float left
    margin-left 120px
    margin-top 100px
    display block
.page-two__h3
    font-family "Open Sans"
    font-weight 300
    font-size 1.2em
    text-align center
    color white
    margin 0
    font-style italic
    text-shadow 1px 1px 4px #000
.page-two__h1
    padding-top 20%
    font-family "Roboto"
    font-weight 700
    font-size 5em
    text-align center
    color white
    margin 0
    text-shadow 2px 2px 8px #000
    padding-bottom 50px
.page-two__ul
    font-family "Roboto"
    font-weight 400
    color white
    text-shadow 1px 1px 3px #000
    margin 0 auto
    width 600px
    padding-top 50px
    line-height 2em
.parallaxParent
    height 100vh
    overflow hidden
.parallaxParent > *
    height: 200%
    position: relative
    top: -100%
